<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button12"
        android:layout_width="93dp"
        android:layout_height="41dp"
        android:text="logout"
        tools:layout_editor_absoluteX="495dp"
        tools:layout_editor_absoluteY="263dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Did you report a missing/broken tool in your Borrow Progress?"
        tools:layout_editor_absoluteX="95dp"
        tools:layout_editor_absoluteY="26dp" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="354dp"
        android:layout_height="32dp"
        android:text="No, everything was fine"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="78dp" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="354dp"
        android:layout_height="wrap_content"
        android:text="Yes, but all tools were fixed/replaced by a staff"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="117dp" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="356dp"
        android:layout_height="wrap_content"
        android:text="Yes, but weren't fixed/replaced"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="161dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Please check all your tools"
        tools:layout_editor_absoluteX="193dp"
        tools:layout_editor_absoluteY="193dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="125dp"
        android:layout_height="wrap_content"
        android:text="Content are fine and complete"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="215dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="141dp"
        android:layout_height="76dp"
        android:text="Sorry, i lost or broke some tools"
        tools:layout_editor_absoluteX="293dp"
        tools:layout_editor_absoluteY="219dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="93dp"
        android:layout_height="41dp"
        android:text="Back"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="263dp" />

</android.support.constraint.ConstraintLayout>